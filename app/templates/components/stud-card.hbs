{{#layout-push direction='col' classNames='stud_card' }}
  {{#layout-push direction='row' split=true}}
        <span class="stud_card_name">{{stud.name}}</span>
        <span class="stud_card_other">
            <span class="stud_card_other_sex">
                {{#if isFemale}}
                    女 ♀
                {{else}}
                    男 ♂
                {{/if}}
            </span>
        <span class="stud_card_other_age">{{stud.age}}岁</span>
        </span>
  {{/layout-push}}
  {{#layout-push direction='col'}}
      {{#layout-push direction='row' split=false}}
        <img src="../images/icon_phone@2x.png" alt="" class="tel_icon"><span class="stud_card_phone">{{stud.contact}}</span>
      {{/layout-push}}
      {{#layout-push direction='row' split=true}}
        <span></span><span class="stud_card_from">来自小程序</span>
      {{/layout-push}}
  {{/layout-push}}

{{/layout-push }}

{{yield}}
