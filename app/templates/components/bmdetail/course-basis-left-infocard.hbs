{{yield}}

{{#layout-push direction='row' mid=true classNames='alt_course_detail_line'}}
    {{#layout-push direction='row' mid=true classNames='alt_course_detail_half'}}
        {{#layout-push direction='col' mid=true }}
            {{#if haveAge}}
                {{#layout-push direction='row' mid=true }}
                    <span class='alt_course_detail_perperty'>{{infoCardData.alb}}-{{infoCardData.aub}} 岁</span>
                {{/layout-push}}
            {{else}}
                {{#layout-push direction='row' mid=true }}
                    <span class='alt_course_detail_perperty'>不限年龄</span>
                {{/layout-push}}
            {{/if}}

            {{#layout-push direction='row' mid=true }}
                <span class='alt_course_detail_perperty_des'>适应年龄</span>
            {{/layout-push}}
        {{/layout-push}}
    {{/layout-push}}
    <div class="border-span"></div>
    {{#layout-push direction='row' mid=true classNames='alt_course_detail_half'}}
        {{#layout-push direction='col' mid=true }}
            {{#if haveLength}}
                {{#layout-push direction='row' mid=true }}
                    <span class='alt_course_detail_perperty'>{{infoCardData.length}} 分钟</span>
                {{/layout-push}}
            {{else}}
                {{#layout-push direction='row' mid=true }}
                    <span class='alt_course_detail_perperty'>不限时长</span>
                {{/layout-push}}
            {{/if}}

            {{#layout-push direction='row' mid=true }}
                <span class='alt_course_detail_perperty_des'>课程时长</span>
            {{/layout-push}}
        {{/layout-push}}
    {{/layout-push}}
{{/layout-push}}
{{!-- <div class='alt_course_deatil_split_line'> </div> --}}
