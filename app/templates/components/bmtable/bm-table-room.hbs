        {{#each titles as |item|}}
            <th class='ib_table_item'>{{item}}</th>
        {{/each}}
    </tr>
    {{bmtable/bm-table-inner type="room" listData=rooms onEditRoomClick=(action "onEditRoomClick") onDeleteRoomClick=(action "onDeleteRoomClick")}}
</table>