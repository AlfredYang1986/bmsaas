{{!-- {{#layout-push direction='row' split=true classNames='alt_course_panel alt_course_top_panel'}}
    {{#layout-push direction='col' mid=true}}
        <span class='alt_course_title'>课程详情</span>
    {{/layout-push}}
    {{#layout-push direction='col' mid=true}}
        {{yield}}
    {{/layout-push}}
{{/layout-push}} --}}

{{yield}}
{{#layout-push direction='row' split=true}}
    {{#layout-push direction='col' classNames='alt_course_left_panel'}}
        {{#layout-push direction='col' classNames='alt_course_panel'}}
            {{!-- {{#layout-push direction='row' mid=true classNames='alt_course_deltail_title'}}
                {{session.title}}
            {{/layout-push}}
            {{#layout-push direction='row' mid=true classNames='alt_course_detail_cat'}}
                {{session.Cate.title}}·{{session.Cate.subtitle}}
            {{/layout-push}} --}}
            {{!-- {{#layout-push direction='row' mid=true classNames='alt_course_detail_line'}}
                {{#layout-push direction='row' mid=true classNames='alt_course_detail_half'}}
                    {{#layout-push direction='col' mid=true }}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty'>{{session.alb}}-{{session.aub}} 岁</span>
                        {{/layout-push}}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty_des'>适应年龄</span>
                        {{/layout-push}}
                    {{/layout-push}}
                {{/layout-push}}
                {{#layout-push direction='row' mid=true classNames='alt_course_detail_half'}}
                    {{#layout-push direction='col' mid=true }}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty'>{{session.level}}</span>
                        {{/layout-push}}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty_des'>等级</span>
                        {{/layout-push}}
                    {{/layout-push}}
                {{/layout-push}}
            {{/layout-push}}
            <div class='alt_course_deatil_split_line'> </div>
            {{#layout-push direction='row' mid=true classNames='alt_course_detail_line'}}
                {{#layout-push direction='row' mid=true classNames='alt_course_detail_half'}}
                    {{#layout-push direction='col' mid=true }}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty'>{{session.count}} 课时</span>
                        {{/layout-push}}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty_des'>预计课时数</span>
                        {{/layout-push}}
                    {{/layout-push}}
                {{/layout-push}}
                {{#layout-push direction='row' mid=true classNames='alt_course_detail_half'}}
                    {{#layout-push direction='col' mid=true }}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty'>{{session.length}} 分钟</span>
                        {{/layout-push}}
                        {{#layout-push direction='row' mid=true }}
                            <span class='alt_course_detail_perperty_des'>课程时长</span>
                        {{/layout-push}}
                    {{/layout-push}}
                {{/layout-push}}
            {{/layout-push}} --}}
            {{bmdetail/course-basis-left-title
                title=session.title
                subTitle=session.Cate.title
                subTitleBehind=session.Cate.subtitle}}
            {{bmdetail/course-basis-left-infocard infoCardData=session}}
            {{#if (bmcom/bm-nil-obj session)}}
            {{else}}
                {{bmcom/bm-pic-jigsaw picData=session.Tagimgs}}
            {{/if}}
        {{/layout-push}}
    {{/layout-push}}
    {{#layout-push direction='col' classNames='alt_course_right_panel_group'}}
        {{#layout-push direction='col' classNames='alt_course_panel alt_course_right_panel'}}
            {{bmdetail/course-basis-right-info label="体验内容" content=session.description}}
            {{bmdetail/course-basis-right-info label="与孩子互动" content=session.play_children}}
            {{bmdetail/course-basis-right-info label="将为宝贝提供" content=session.inc}}

            {{!-- {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>体验内容</span>
                {{#if (eq session.description "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    <span class='alt_right_panel_perperty_des'>{{session.description}}</span>
                {{/if}}
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>与孩子互动</span>
                {{#if (eq session.play_children "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    <span class='alt_right_panel_perperty_des'>{{session.play_children}}</span>
                {{/if}}
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>将为宝贝提供</span>
                {{#if (eq session.inc "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    <span class='alt_right_panel_perperty_des'>{{session.inc}}</span>
                {{/if}}
            {{/layout-push}} --}}
        {{/layout-push}}

        {{#layout-push direction='col' classNames='alt_course_panel alt_course_right_panel'}}
            {{bmdetail/course-basis-right-info label="宝贝将获得" content=session.acquisition}}
            {{#if (bmeq session.accompany 1)}}
                {{bmdetail/course-basis-right-info label="陪同说明" content="需家长陪同"}}
            {{else}}
                {{bmdetail/course-basis-right-info label="陪同说明" content="不需家长陪同"}}
            {{/if}}
            {{bmdetail/course-basis-right-info label="参与者需自带" content=session.carrying}}
            {{bmdetail/course-basis-right-info label="须知" content=session.notice}}

            {{!-- {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>宝贝将获得</span>
                {{#if (eq session.acquisition "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    <span class='alt_right_panel_perperty_des'>{{session.acquisition}}</span>
                {{/if}}
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>陪同说明</span>
                {{#if (eq session.accompany "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    {{#if (eq session.accompany 1)}}
                        <span class='alt_right_panel_perperty_des'>需家长陪同</span>
                    {{else}}
                        <span class='alt_right_panel_perperty_des'>不需家长陪同</span>
                    {{/if}}
                {{/if}}
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>参与者需自带</span>
                {{#if (eq session.carrying "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    <span class='alt_right_panel_perperty_des'>{{session.carrying}}</span>
                {{/if}}
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>须知</span>
                {{#if (eq session.notice "")}}
                    <span class='alt_right_panel_perperty_des'>请上传信息</span>
                {{else}}
                    <span class='alt_right_panel_perperty_des'>{{session.notice}}</span>
                {{/if}}
            {{/layout-push}} --}}
        {{/layout-push}}

        {{!-- {{#layout-push direction='col' classNames='alt_course_panel alt_course_right_panel'}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>教育理念和模式</span>
                {{#if (eqsadasd session "".tags)}}
                    {{#layout-push direction='row' mid=true}}
                        <span class='alt_right_panel_perperty_des'>请上传信息</span>
                    {{/layout-push}}
                {{else}}
                    {{#layout-push direction='row'}}
                        {{#each session.tags as |item|}}
                            <span class='alt_yard_detail_tag'>{{item}}</span>
                        {{/each}}
                    {{/layout-push}}
                {{/if}}
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>课程目标</span>
                <span class='alt_right_panel_perperty_des'>{{session.target}}</span>
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>课程计划</span>
                <span class='alt_right_panel_perperty_des'>{{session.planning}}</span>
            {{/layout-push}}
            {{#layout-push direction='col' classNames='alt_course_detail_panel'}}
                <span class='alt_right_panel_perperty_title'>课程内容</span>
                <span class='alt_right_panel_perperty_des'>{{session.description}}</span>
            {{/layout-push}}
        {{/layout-push}} --}}

    {{/layout-push}}
{{/layout-push}}
